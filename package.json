    "nodemon": "^2.0.3",
    "on-covid-19": "^0.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "babel-jest": "^25.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.2.0"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
